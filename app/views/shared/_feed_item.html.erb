<li id="<%= feed_item.id %>">
  <%= link_to gravatar_for(feed_item.user), feed_item.user %>
  <span class="user">
    <%= link_to feed_item.user.name, feed_item.user %>
  </span>
  <span class="content">Your Input: <%= feed_item.in_url %></span>
  <span class="content">Destination: <%= feed_item.out_url %></span>
  <span class="content">Counter Analytics: <%= feed_item.counter %></span>
  <span class="timestamp">
    Posted <%= time_ago_in_words(feed_item.created_at) %> ago.
  </span>
</li>